{
  "name": "testing-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "commit-msg": "commitlint --edit $1",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "lint": "eslint --ext .ts --fix --quiet",
    "lint:all": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "tsc": "tsc -p ./tsconfig.build.json --noEmit --pretty",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node --cwdMode --project ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./migrations/lib/dataSource.ts",
    "migration:create": "migrationCreate() { ts-node  -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create \"./migrations/$1\" && eslint \"migrations/*-$1.ts\" --fix; }; migrationCreate",
    "migration:generate": "migrationGenerate() { npm run typeorm:cli migration:generate \"./migrations/$1\" && eslint \"migrations/*-$1.ts\" --fix; }; migrationGenerate",
    "migration:show": "npm run typeorm:cli migration:show",
    "migration:run": "npm run typeorm:cli migration:run",
    "migration:run:prod": "node ./node_modules/typeorm/cli.js -d ./dist/migrations/lib/dataSource.js migration:run",
    "migration:revert": "npm run typeorm:cli migration:revert",
    "migration:revert:prod": "node ./node_modules/typeorm/cli.js -d ./dist/migrations/lib/dataSource.js migration:revert",
    "migration:init": "rm -rf ./migrations && npm run schema:drop && npm run migration:generate Initial && npm run migration:run",
    "schema:diff": "npm run typeorm:cli schema:log",
    "schema:drop": "npm run typeorm:cli schema:drop",
    "schema:sync": "npm run typeorm:cli schema:sync",
    "schema:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/dist/cli/index.js db:create",
    "schema:seed": "ts-node -r tsconfig-paths/register ./test/seedDatabase.ts",
    "env:up": "docker-compose up -d",
    "env:down": "docker-compose down",
    "release": "standard-version"
  },
  "dependencies": {
    "@nestjs/axios": "2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "2.2.0",
    "@nestjs/swagger": "6.1.2",
    "@nestjs/throttler": "4.2.1",
    "@nestjs/typeorm": "9.0.1",
    "aws-sdk": "2.1313.0",
    "axios": "1.4.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "csv-writer": "1.6.0",
    "ftps": "1.2.0",
    "helmet": "6.0.0",
    "joi": "17.6.4",
    "lodash": "4.17.21",
    "mime": "3.0.0",
    "moment": "2.29.4",
    "moment-timezone": "0.5.43",
    "nestjs-pino": "3.1.1",
    "pg": "8.8.0",
    "pino": "8.7.0",
    "pino-pretty": "9.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sanitize-html": "2.10.0",
    "sharp": "0.31.3",
    "sql-formatter": "11.0.2",
    "standard-version": "9.5.0",
    "typeorm": "0.3.16",
    "typeorm-extension": "2.8.1",
    "typeorm-naming-strategies": "4.1.0",
    "uuid": "9.0.0",
    "vk-io": "4.8.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/ftps": "1.2.0",
    "@types/jest": "28.1.8",
    "@types/lodash": "4.14.188",
    "@types/multer": "1.4.7",
    "@types/node": "^16.0.0",
    "@types/sanitize-html": "2.9.0",
    "@types/sharp": "0.31.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "9.0.0",
    "@types/validator": "13.9.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "commitlint": "^17.1.2",
    "csv-parser": "3.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "^8.0.1",
    "jest": "28.1.3",
    "lint-staged": "14.0.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
